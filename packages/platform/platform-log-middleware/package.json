{
  "name": "@tsed/platform-log-middleware",
  "version": "6.132.0",
  "description": "Log request middleware module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2"
  },
  "dependencies": {
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@tsed/di": "6.132.0",
    "@tsed/platform-middlewares": "6.132.0",
    "@tsed/platform-params": "6.132.0"
  },
  "peerDependencies": {
    "@tsed/di": "^6.132.0",
    "@tsed/platform-middlewares": "^6.132.0",
    "@tsed/platform-params": "^6.132.0"
  },
  "peerDependenciesMeta": {
    "@tsed/di": {
      "optional": false
    },
    "@tsed/platform-middlewares": {
      "optional": false
    },
    "@tsed/platform-params": {
      "optional": false
    }
  }
}