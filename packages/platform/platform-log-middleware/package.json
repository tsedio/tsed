  "dependencies": {
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/barrels": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/platform-middlewares": "workspace:*",
    "@tsed/platform-params": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "eslint": "^8.57.0",
    "typescript": "5.6.2",
    "vitest": "2.1.1"
  },
  "peerDependencies": {
    "@tsed/di": "8.0.0-alpha.5",
    "@tsed/platform-middlewares": "8.0.0-alpha.5",
    "@tsed/platform-params": "8.0.0-alpha.5"
  },
  "peerDependenciesMeta": {
    "@tsed/di": {
      "optional": false
    },
    "@tsed/platform-middlewares": {
      "optional": false
    },
    "@tsed/platform-params": {
      "optional": false
    }
  }
