{
  "name": "@tsed/platform-log-middleware",
  "version": "7.61.2",
  "description": "Log request middleware module for Ts.ED Framework",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "dependencies": {
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/di": "workspace:*",
    "@tsed/platform-middlewares": "workspace:*",
    "@tsed/platform-params": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "barrelsby": "^2.8.1",
    "eslint": "^8.12.0",
    "jest": "^29.2.0"
  },
  "peerDependencies": {
    "@tsed/di": "7.61.2",
    "@tsed/platform-middlewares": "7.61.2",
    "@tsed/platform-params": "7.61.2"
  },
  "peerDependenciesMeta": {
    "@tsed/di": {
      "optional": false
    },
    "@tsed/platform-middlewares": {
      "optional": false
    },
    "@tsed/platform-params": {
      "optional": false
    }
  }
}
