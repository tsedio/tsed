{
  "name": "@tsed/platform-test-sdk",
  "version": "7.42.0",
  "description": "Package to test platform adapter integration with Ts.ED",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts",
    "barrels": "yarn barrelsby --delete -d ./src -d ./src/interfaces -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json",
    "lint": "eslint '**/*.{ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "keywords": [
    "Ts.ED",
    "Testing",
    "sdk",
    "Platform",
    "Adapter"
  ],
  "dependencies": {
    "@tsed/ajv": "7.42.0",
    "@tsed/common": "7.42.0",
    "@tsed/core": "7.42.0",
    "@tsed/di": "7.42.0",
    "@tsed/json-mapper": "7.42.0",
    "@tsed/logger": "6.6.3",
    "@tsed/platform-cache": "7.42.0",
    "@tsed/platform-params": "7.42.0",
    "@tsed/platform-router": "7.42.0",
    "@tsed/platform-views": "7.42.0",
    "@tsed/schema": "7.42.0",
    "@tsed/swagger": "7.42.0",
    "ajv": "^8.12.0",
    "axios": "^1.3.0",
    "read-pkg-up": "7.0.1"
  },
  "devDependencies": {
    "@tsed/eslint": "7.39.1",
    "@tsed/typescript": "7.39.1",
    "eslint": "^8.12.0"
  },
  "peerDependencies": {}
}