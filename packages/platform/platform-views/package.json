{
  "name": "@tsed/platform-views",
  "version": "6.102.4",
  "description": "Views module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
