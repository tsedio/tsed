  "devDependencies": {
    "@tsed/core": "6.102.4",
    "@tsed/di": "6.102.4",
    "@tsed/engines": "^1.1.5",
    "@tsed/exceptions": "6.102.4",
    "@tsed/schema": "6.102.4",
    "@types/consolidate": "0.14.0",
    "@types/ejs": "3.1.0"
  },
  "peerDependencies": {
    "@tsed/core": "^6.102.4",
    "@tsed/di": "^6.102.4",
    "@tsed/exceptions": "^6.102.4",
    "@tsed/schema": "^6.102.4",
    "@tsed/engines": "^1.1.5"
  },
  "peerDependenciesMeta": {
