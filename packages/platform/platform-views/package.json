{
  "name": "@tsed/platform-views",
  "version": "7.67.3",
  "description": "Views module for Ts.ED Framework",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "dependencies": {
    "@tsed/engines": "workspace:*",
    "ejs": "^3.1.9",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/engines": "workspace:*",
    "@tsed/exceptions": "workspace:*",
    "@tsed/schema": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/ejs": "3.1.5",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "@tsed/core": "7.67.3",
    "@tsed/di": "7.67.3",
    "@tsed/engines": "7.67.3",
    "@tsed/exceptions": "7.67.3",
    "@tsed/schema": "7.67.3"
  },
  "peerDependenciesMeta": {
