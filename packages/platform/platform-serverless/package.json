{
  "name": "@tsed/platform-serverless",
  "version": "7.57.0",
  "description": "Platform serverless module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test jest && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json",
    "lint": "eslint '**/*.{ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "dependencies": {
    "@tsed/core": "7.57.0",
    "@tsed/exceptions": "7.57.0",
    "@tsed/json-mapper": "7.57.0",
    "@tsed/platform-exceptions": "7.57.0",
    "@tsed/platform-params": "7.57.0",
    "@tsed/schema": "7.57.0",
    "encodeurl": "^1.0.2",
    "mime": "^3.0.0",
    "tslib": "2.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsed/eslint": "7.44.1",
    "@tsed/platform-serverless-testing": "7.57.0",
    "@tsed/typescript": "7.44.1",
    "@types/aws-lambda": "^8.10.83",
    "@types/encodeurl": "^1.0.0",
    "@types/mime": "^3.0.1",
    "eslint": "^8.12.0",
    "find-my-way": "^7.0.0"
  },
  "peerDependencies": {
    "@tsed/di": "^7.57.0",
    "@tsed/logger": ">=6.2.2"
  },
  "peerDependenciesMeta": {
    "@tsed/di": {
      "optional": false
    },
    "@tsed/logger": {
      "optional": false
    },
    "find-my-way": {
      "optional": true
    }
  }
}
