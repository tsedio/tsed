{
  "name": "@tsed/platform-serverless",
  "version": "7.10.0-rc.6",
  "description": "Platform serverless module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@tsed/core": "7.10.0-rc.6",
    "@tsed/exceptions": "7.10.0-rc.6",
    "@tsed/json-mapper": "7.10.0-rc.6",
    "@tsed/platform-exceptions": "7.10.0-rc.6",
    "@tsed/platform-params": "7.10.0-rc.6",
    "@tsed/schema": "7.10.0-rc.6",
    "encodeurl": "^1.0.2",
    "mime": "^3.0.0",
    "tslib": "2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsed/platform-serverless-testing": "7.10.0-rc.6",
    "@types/aws-lambda": "^8.10.83",
    "@types/encodeurl": "^1.0.0",
    "@types/mime": "^3.0.1",
    "find-my-way": "^7.0.0"
  },
  "peerDependencies": {
    "@tsed/di": "^7.10.0-rc.6",
    "@tsed/logger": ">=6.2.2"
  },
  "peerDependenciesMeta": {
    "@tsed/di": {
      "optional": false
    },
    "@tsed/logger": {
      "optional": false
    },
    "find-my-way": {
      "optional": true
    }
  }
}