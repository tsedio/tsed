{
  "name": "@tsed/platform-serverless",
  "version": "7.0.0-beta.1",
  "description": "Platform serverless module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 --coverage"
  },
  "dependencies": {
    "@tsed/core": "7.0.0-beta.1",
    "@tsed/di": "7.0.0-beta.1",
    "@tsed/exceptions": "7.0.0-beta.1",
    "@tsed/json-mapper": "7.0.0-beta.1",
    "@tsed/logger": "^6.0.0",
    "@tsed/platform-exceptions": "7.0.0-beta.1",
    "@tsed/platform-params": "7.0.0-beta.1",
    "@tsed/schema": "7.0.0-beta.1",
    "encodeurl": "^1.0.2",
    "mime": "^2.5.2",
    "tslib": "2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsed/platform-serverless-testing": "7.0.0-beta.1",
    "@types/aws-lambda": "^8.10.83",
    "@types/encodeurl": "^1.0.0",
    "@types/mime": "^2.0.3"
  },
  "peerDependencies": {}
}