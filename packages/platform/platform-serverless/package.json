    "mime": "^4.0.4",
    "tslib": "2.7.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@tsed/barrels": "workspace:*",
    "@tsed/platform-serverless-testing": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/aws-lambda": "^8.10.145",
    "@types/encodeurl": "^1.0.2",
    "@types/mime": "^4.0.0",
    "aws-lambda": "1.0.7",
    "eslint": "9.12.0",
    "find-my-way": "^9.1.0",
    "typescript": "5.4.5",
    "vitest": "2.1.2"
  },
  "peerDependencies": {
    "@tsed/di": "8.0.0-alpha.9",
