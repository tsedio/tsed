{
  "name": "@tsed/platform-serverless",
  "version": "6.114.5",
  "description": "Platform serverless module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@tsed/core": "6.114.5",
    "@tsed/exceptions": "6.114.5",
    "@tsed/json-mapper": "6.114.5",
    "@tsed/platform-exceptions": "6.114.5",
    "@tsed/platform-params": "6.114.5",
    "@tsed/schema": "6.114.5",
    "encodeurl": "^1.0.2",
    "mime": "^2.5.2",
    "tslib": "2.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsed/platform-serverless-testing": "6.114.5",
    "@types/aws-lambda": "^8.10.83",
    "@types/encodeurl": "^1.0.0",
    "@types/mime": "^2.0.3"
  },
  "peerDependencies": {
    "@tsed/di": "^6.114.5",
    "@tsed/logger": ">=6.1.1"
  },
  "peerDependenciesMeta": {
    "@tsed/di": {
      "optional": false
    },
    "@tsed/logger": {
      "optional": false
    }
  }
}