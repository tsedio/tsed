{
  "name": "@tsed/passport",
  "description": "Passport package for Ts.ED framework",
  "type": "commonjs",
  "version": "7.79.4",
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels && yarn build:ts",
    "barrels": "barrels",
    "test": "jest --max-workers=4",
    "start": "ts-node src/index.ts",
    "build:ts": "tsc --build tsconfig.json",
    "test:ci": "jest --max-workers=2 && jest-coverage-thresholds-bumper"
  },
  "dependencies": {
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/barrels": "workspace:*",
    "@tsed/common": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/platform-router": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/passport": "1.0.16",
    "@types/passport-http": "0.3.11",
    "@types/passport-local": "1.0.38",
    "@types/passport-strategy": "0.2.38",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "passport": "0.7.0",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "passport-strategy": "1.0.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@tsed/common": "7.79.4",
    "passport": ">=0.4.1"
  },
  "peerDependenciesMeta": {
    "@tsed/common": {
      "optional": false
    }
  }
}
