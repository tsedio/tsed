{
  "name": "@tsed/oidc-provider",
  "version": "6.100.3",
  "description": "OIDC provider package for Ts.ED framework",
  "private": false,
  "contributors": [
    {
      "name": "romakita"
    },
    {
      "name": "stefanvanherwijnen"
    }
  ],
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "start": "ts-node -r tsconfig-paths/register test/app/index.ts",
    "test": "cross-env NODE_ENV=test nyc mocha"
  },
  "dependencies": {
    "@tsed/adapters": "6.100.3",
    "express-urlrewrite": "^1.4.0",
    "jose2": "npm:jose@^2.0.4",
    "koa-mount": "^4.0.0",
    "koa-rewrite": "^3.0.1",
    "lowdb": "1.0.0",
    "tslib": "2.2.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@tsed/common": "6.100.3",
    "@tsed/core": "6.100.3",
    "@tsed/di": "6.100.3",
    "@tsed/exceptions": "6.100.3",
    "@types/lowdb": "1.0.9",
    "@types/oidc-provider": "^7.1.1",
    "@types/uuid": "8.3.0",
    "oidc-provider": "7.10.1"
  },
  "peerDependencies": {
    "@tsed/common": "^6.100.3",
    "@tsed/core": "^6.100.3",
    "@tsed/di": "^6.100.3",
    "@tsed/exceptions": "^6.100.3",
    "@tsed/json-mapper": "^6.100.3",
    "@tsed/logger": ">=6.0.0",
    "@tsed/openspec": "^6.100.3",
    "@tsed/schema": "^6.100.3",
    "oidc-provider": ">=7.3.0"
  }
}