{
  "name": "@tsed/oidc-provider-plugin-wildcard-redirect-uri",
  "version": "7.61.2",
  "description": "A plugin to support wildcard in the redirect uri whitelist",
  "contributors": [
    {
      "name": "romakita"
    }
  ],
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts",
    "start": "ts-node test/app/index.ts",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "dependencies": {
    "wildcard": "2.0.0"
  },
  "devDependencies": {
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/oidc-provider": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/uuid": "8.3.4",
    "barrelsby": "^2.8.1",
    "eslint": "^8.12.0",
    "jest": "^29.2.0"
  },
  "peerDependencies": {
    "@tsed/core": "7.61.2",
    "@tsed/di": "7.61.2",
    "@tsed/logger": ">=6.2.2",
    "@tsed/oidc-provider": "7.61.2",
    "oidc-provider": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "@tsed/core": {
      "optional": false
    },
    "@tsed/di": {
      "optional": false
    },
    "@tsed/oidc-provider": {
      "optional": false
    }
  }
}
