{
  "name": "@tsed/testcontainers-mongo",
  "description": "A module to test Ts.ED code that use MongoDB as database using Test Containers",
  "type": "module",
  "version": "8.0.0-alpha.9",
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    },
    "./vitest/setup": {
      "types": "./lib/types/setup/vi.setup.d.ts",
      "import": "./lib/esm/setup/vi.setup.js",
      "default": "./lib/esm/setup/vi.setup.js"
    }
  },
  "scripts": {
    "build": "yarn build:ts",
    "build:ts": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "semver": "^7.6.3",
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@testcontainers/mongodb": "^10.13.2",
    "@tsed/barrels": "workspace:*",
    "@tsed/common": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "eslint": "9.12.0",
    "mongodb": "6.9.0",
    "testcontainers": ">=10.11.0",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@testcontainers/mongodb": ">=10.11.0",
    "@tsed/common": "8.0.0-alpha.9",
    "@tsed/core": "8.0.0-alpha.9",
    "@tsed/di": "8.0.0-alpha.9",
    "mongodb": ">=6",
    "testcontainers": ">=10.11.0"
  }
}
