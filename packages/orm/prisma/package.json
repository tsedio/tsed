{
  "name": "@tsed/prisma",
  "version": "7.21.1",
  "description": "Generate Ts.ED JsonSchema based on Prisma models",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "bin": {
    "tsed-prisma": "lib/cjs/generator.js"
  },
  "scripts": {
    "build": "rm -rf lib && yarn build:ts",
    "test": "cross-env NODE_ENV=test yarn jest && jest-coverage-thresholds-bumper",
    "generate:postgres": "cd test/postgres && prisma -v && prisma generate",
    "generate:mongo": "cd test/mongo && prisma -v && prisma generate",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json",
    "lint": "eslint '**/*.{ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "private": false,
  "dependencies": {
    "@prisma/generator-helper": "^4.0.0",
    "@prisma/internals": "^4.0.0",
    "change-case": "^4.1.2",
    "fs-extra": "^10.1.0",
    "pluralize": "^8.0.0",
    "ts-morph": "^12.0.0",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.0.0",
    "@tsed/core": "7.21.1",
    "@tsed/di": "7.21.1",
    "@tsed/eslint": "7.21.1",
    "@tsed/json-mapper": "7.21.1",
    "@tsed/schema": "7.21.1",
    "@tsed/typescript": "7.21.1",
    "@types/change-case": "^2.3.1",
    "@types/pluralize": "0.0.29",
    "eslint": "^8.12.0",
    "prisma": "^4.0.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=4.0.0",
    "@tsed/core": "^7.21.1",
    "@tsed/di": "^7.21.1",
    "@tsed/json-mapper": "^7.21.1",
    "@tsed/schema": "^7.21.1"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json schema",
    "JsonSchema",
    "class",
    "classes",
    "tsed",
    "prisma"
  ]
}