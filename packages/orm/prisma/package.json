{
  "name": "@tsed/prisma",
  "version": "7.63.0",
  "description": "Generate Ts.ED JsonSchema based on Prisma models",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "bin": {
    "tsed-prisma": "lib/cjs/generator.js"
  },
  "scripts": {
    "build": "rm -rf lib && yarn build:ts",
    "test": "cross-env NODE_ENV=test yarn jest && jest-coverage-thresholds-bumper",
    "generate:postgres": "cd test/postgres && prisma -v && prisma generate",
    "generate:mongo": "cd test/mongo && prisma -v && prisma generate",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.0.0",
    "@prisma/internals": "^4.0.0",
    "change-case": "^4.1.2",
    "fs-extra": "^11.1.1",
    "pluralize": "^8.0.0",
    "ts-morph": "^12.0.0",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@prisma/client": "^4.0.0",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/json-mapper": "workspace:*",
    "@tsed/schema": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/change-case": "^2.3.1",
    "@types/pluralize": "0.0.29",
    "barrelsby": "^2.8.1",
    "eslint": "^8.12.0",
    "jest": "^29.2.0",
    "prisma": "^4.0.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=4.0.0",
    "@tsed/core": "7.63.0",
    "@tsed/di": "7.63.0",
    "@tsed/json-mapper": "7.63.0",
    "@tsed/schema": "7.63.0"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json schema",
    "JsonSchema",
    "class",
    "classes",
    "tsed",
    "prisma"
  ]
}
