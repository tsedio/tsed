    "@tsed/typescript": "workspace:*",
    "eslint": "^8.57.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@tsed/common": "7.83.0",
    "@tsed/core": "7.83.0",
    "@tsed/mongoose": "7.83.0",
    "mongoose": ">=6.0.0"
