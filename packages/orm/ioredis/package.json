    "@types/ioredis-mock": "^5.6.0",
    "eslint": "^8.57.0",
    "ioredis": "5.3.2",
    "ioredis-mock": "8.9.0",
    "typescript": "5.6.2",
    "vitest": "2.1.1"
  },
  "peerDependencies": {
    "@tsed/core": "8.0.0-alpha.5",
    "@tsed/di": "8.0.0-alpha.5",
    "ioredis": ">=5.2.3",
    "ioredis-mock": ">=8.2.2"
