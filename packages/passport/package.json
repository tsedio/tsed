{
  "name": "@tsed/passport",
  "version": "6.49.1",
  "description": "Passport package for Ts.ED framework",
  "keywords": [
    "tsed",
    "express",
    "passportjs",
    "security",
    "jwt",
    "facebook",
    "google",
    "oauth2"
  ],
  "source": "./src/index.ts",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "exports": {
    "require": "./lib/index.js",
    "default": "./lib/index.modern.js"
  },
  "scripts": {
    "build": "microbundle --target node --no-compress --format modern,cjs --tsconfig ./tsconfig.compile.json",
    "start": "ts-node src/index.ts"
  },
  "dependencies": {
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@tsed/common": "6.49.1",
    "@tsed/core": "6.49.1",
    "@tsed/di": "6.49.1",
    "@types/passport": "1.0.4",
    "@types/passport-http": "0.3.8",
    "@types/passport-local": "1.0.33",
    "@types/passport-strategy": "0.2.35",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "passport-strategy": "1.0.0"
  },
  "private": false,
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "peerDependencies": {
    "@tsed/common": "^6.48.6",
    "@tsed/core": "^6.48.6",
    "@tsed/di": "^6.48.6",
    "@tsed/schema": "^6.48.6",
    "@tsed/json-mapper": "^6.48.6",
    "@tsed/openspec": "^6.48.6",
    "@tsed/logger": "^5.13.3",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0"
  }
}