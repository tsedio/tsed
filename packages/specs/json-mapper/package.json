{
  "name": "@tsed/json-mapper",
  "version": "7.11.0",
  "description": "Json mapper module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/json-mapper.umd.min.js",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "browser": "./lib/browser/json-mapper.umd.min.js",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels && yarn run build:esm && yarn run build:cjs && yarn run build:browser",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "build:browser": "webpack",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json mapper",
    "mapper",
    "serialization",
    "deserialization",
    "class",
    "classes",
    "tsed"
  ],
  "dependencies": {
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@tsed/core": "7.11.0",
    "@tsed/schema": "7.11.0"
  },
  "peerDependencies": {
    "@tsed/core": "^7.11.0",
    "@tsed/schema": "^7.11.0"
  },
  "peerDependenciesMeta": {
    "@tsed/core": {
      "optional": false
    },
    "@tsed/exceptions": {
      "optional": false
    },
    "@tsed/schema": {
      "optional": false
    }
  }
}