{
  "extends": "@tsed/typescript/tsconfig.node.json",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "rootDir": "..",
    "declaration": false,
    "composite": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.json", "../**/*.ts", "../**/*.json"],
  "exclude": ["node_modules", "lib"],
  "references": [
    {
      "path": "../"
    }
  ]
}
