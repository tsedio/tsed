{
  "name": "@tsed/schema",
  "version": "7.67.4",
  "description": "JsonSchema module for Ts.ED Framework",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/schema.umd.min.js",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "browser": "./lib/browser/schema.umd.min.js",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels && yarn build:ts && yarn run build:browser",
    "build:browser": "webpack",
    "barrels": "yarn barrelsby --config .barrelsby.json",
    "test": "jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json schema",
    "JsonSchema",
    "class",
    "classes",
    "tsed"
  ],
  "dependencies": {
    "@tsed/openspec": "workspace:*",
    "change-case": "^4.1.2",
    "fs-extra": "^11.2.0",
    "json-schema": "0.4.0",
    "picomatch": "2.3.1",
    "statuses": ">=2.0.1",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "10.1.0",
    "@tsed/core": "workspace:*",
    "@tsed/openspec": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/fs-extra": "11.0.4",
    "@types/json-schema": "7.0.15",
    "@types/picomatch": "2.3.3",
    "@types/statuses": "2.0.5",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "picomatch-browser": "^2.2.6"
  },
  "peerDependencies": {
    "@tsed/core": "7.67.4",
    "@tsed/openspec": "7.67.4"
  },
  "peerDependenciesMeta": {
    "@tsed/core": {
      "optional": false
    },
    "@tsed/openspec": {
      "optional": false
    }
  }
