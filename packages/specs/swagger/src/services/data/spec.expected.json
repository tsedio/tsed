{
  "components": {
    "securitySchemes": {
      "calendar_auth": {
        "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
        "flow": "implicit",
        "scopes": {
          "read:calendar": "read your pets",
          "write:calendar": "modify pets in your account"
        },
        "type": "oauth2"
      },
      "global_auth": {
        "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
        "flow": "implicit",
        "scopes": {
          "read:global": "read your pets",
          "write:global": "modify pets in your account"
        },
        "type": "oauth2"
      }
    }
  },
