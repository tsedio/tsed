{
  "name": "@tsed/swagger",
  "version": "7.62.2",
  "description": "Swagger package for Ts.ED framework",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels && yarn build:ts && cp -r ./views ./lib/views",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "start": "ts-node test/app/index.ts",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "dependencies": {
    "@tsed/normalize-path": "workspace:*",
    "@tsed/openspec": "workspace:*",
    "filedirname": "^2.7.0",
    "fs-extra": "11.1.1",
    "micromatch": "4.0.5",
    "swagger-ui-dist": "^4.5.2",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/common": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/json-mapper": "workspace:*",
    "@tsed/openspec": "workspace:*",
    "@tsed/schema": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/swagger-ui-dist": ">=3.30.1",
    "barrelsby": "^2.8.1",
    "eslint": "^8.12.0",
    "jest": "^29.2.0"
  },
  "peerDependencies": {
    "@tsed/common": "7.62.2"
  },
  "peerDependenciesMeta": {
    "@tsed/common": {
      "optional": false
    }
  }
}