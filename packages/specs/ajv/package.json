  "devDependencies": {
    "@tsed/core": "7.18.2",
    "@tsed/di": "7.18.2",
    "@tsed/exceptions": "7.18.2",
    "@tsed/schema": "7.18.2",
    "ajv": "8.11.0",
    "@tsed/typescript": "7.18.2",
    "@tsed/eslint": "7.18.2",
    "eslint": "^8.12.0"
  },
  "peerDependencies": {
    "@tsed/core": "^7.18.2",
    "@tsed/di": "^7.18.2",
    "@tsed/exceptions": "^7.18.2",
    "@tsed/schema": "^7.18.2",
