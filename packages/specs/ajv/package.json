    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2",
    "build:ts": "tsc --build tsconfig.json",
    "lint": "eslint '**/*.{ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix"
