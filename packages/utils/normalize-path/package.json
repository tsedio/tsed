    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "dependencies": {
    "normalize-path": "3.0.0",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/typescript": "workspace:*",
    "@types/normalize-path": "^3.0.2",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
