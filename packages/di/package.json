    "@tsed/schema": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "barrelsby": "^2.8.1",
    "eslint": "^8.12.0",
    "jest": "^29.2.0"
  },
  "peerDependencies": {
    "@tsed/core": "7.63.0",
    "@tsed/logger": ">=6.2.2",
    "@tsed/schema": "7.63.0"
  },
  "peerDependenciesMeta": {
    "@tsed/core": {
      "optional": false
    },
