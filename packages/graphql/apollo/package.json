{
  "name": "@tsed/apollo",
  "version": "7.10.0-rc.4",
  "description": "Apollo package for Ts.ED framework",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "tslib": "2.4.0"
  },
  "private": false,
  "devDependencies": {
    "@tsed/common": "7.10.0-rc.4",
    "@tsed/core": "7.10.0-rc.4",
    "@tsed/di": "7.10.0-rc.4",
    "@tsed/logger": ">=6.2.2",
    "@types/graphql": "14.5.0",
    "apollo-datasource": "^3.2.0",
    "apollo-datasource-rest": "^3.3.0",
    "apollo-server-core": "^3.4.0",
    "apollo-server-express": "^3.4.0",
    "apollo-server-koa": "^3.4.0",
    "apollo-server-testing": "^2.25.2",
    "graphql": "15.7.2"
  },
  "peerDependencies": {
    "@tsed/common": "^7.10.0-rc.4",
    "@tsed/core": "^7.10.0-rc.4",
    "@tsed/di": "^7.10.0-rc.4",
    "@tsed/logger": ">=6.2.2",
    "apollo-datasource": ">=3.0.0",
    "apollo-datasource-rest": ">=3.0.0",
    "apollo-server-core": ">=3.0.0",
    "graphql": ">15.0.0"
  }
}