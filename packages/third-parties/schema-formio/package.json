{
  "name": "@tsed/schema-formio",
  "version": "6.87.1",
  "description": "Transform Ts.ED Schema & JsonSchema to a valid Formio schema",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "exports": {
    "require": "./lib/index.js",
    "default": "./lib/index.modern.js"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json",
    "schema",
    "JsonSchema",
    "formio",
    "class",
    "classes",
    "tsed"
  ],
  "scripts": {
    "build": "microbundle --target node --no-compress --format modern,cjs --tsconfig ./tsconfig.compile.json",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@tsed/formio-types": "6.87.1",
    "change-case": "4.1.2",
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@tsed/core": "6.87.1",
    "@tsed/openspec": "6.87.1",
    "@tsed/schema": "6.87.1"
  },
  "peerDependencies": {
    "@tsed/core": "^6.87.1",
    "@tsed/openspec": "^6.87.1",
    "@tsed/schema": "^6.87.1",
    "formiojs": ">=4.0.0",
    "lodash": ">=4.0.0",
    "moment": ">=2.0.0"
  }
}