{
  "name": "@tsed/schema-formio",
  "description": "Transform Ts.ED Schema & JsonSchema to a valid Formio schema",
  "type": "module",
  "version": "8.0.0-alpha.10",
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/schema-formio.umd.min.js",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "browser": "./lib/browser/schema-formio.umd.min.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels && yarn build:ts && yarn run build:browser",
    "build:browser": "webpack",
    "barrels": "barrels",
    "test": "vitest run",
    "build:ts": "tsc --build tsconfig.json",
    "test:ci": "vitest run --coverage.thresholds.autoUpdate=true"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json",
    "schema",
    "JsonSchema",
    "formio",
    "class",
    "classes",
    "tsed"
  ],
  "dependencies": {
    "@tsed/formio-types": "workspace:*",
    "change-case": "^5.4.4",
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@tsed/barrels": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/openspec": "workspace:*",
    "@tsed/schema": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "eslint": "9.12.0",
    "typescript": "5.4.5",
    "vitest": "2.1.2"
  },
  "peerDependencies": {
    "@tsed/core": "8.0.0-alpha.10",
    "@tsed/openspec": "8.0.0-alpha.10",
    "@tsed/schema": "8.0.0-alpha.10",
    "formiojs": ">=4.0.0",
    "lodash": ">=4.0.0",
    "moment": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "@tsed/core": {
      "optional": false
    },
    "@tsed/openspec": {
      "optional": false
    },
    "@tsed/schema": {
      "optional": false
    },
    "formiojs": {
      "optional": false
    },
    "lodash": {
      "optional": false
    },
    "moment": {
      "optional": false
    }
  }
}
