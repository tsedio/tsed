{
  "name": "@tsed/event-emitter",
  "description": "EventEmitter package for Ts.ED framework",
  "type": "module",
  "version": "8.0.0-alpha.6",
  "contributors": [
    {
      "name": "Oliver Christen"
    }
  ],
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/event-emitter.umd.min.js",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "browser": "./lib/browser/event-emitter.umd.min.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/cjs/index.js"
    }
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts && yarn run build:browser",
    "build:browser": "webpack",
    "barrels": "barrels",
    "test": "vitest run",
    "build:ts": "tsc --build tsconfig.json",
    "test:ci": "vitest run --coverage.thresholds.autoUpdate=true"
  },
  "dependencies": {
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/barrels": "workspace:*",
    "@tsed/common": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "eslint": "^8.57.0",
    "eventemitter2": "^6.4.9",
    "typescript": "5.4.5",
    "vitest": "2.0.4",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "eventemitter2": "^6.4.4"
  }
}
