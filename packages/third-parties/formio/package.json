{
  "name": "@tsed/formio",
  "version": "7.62.2",
  "description": "Formio package for Ts.ED framework",
  "keywords": [
    "Formio",
    "Ts.ED",
    "tsed",
    "TypeScript",
    "decorators",
    "express"
  ],
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "start": "ts-node test/app/index.ts",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 && jest-coverage-thresholds-bumper",
    "build:ts": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json"
  },
  "dependencies": {
    "@tsed/formio-types": "workspace:*",
    "@tsed/normalize-path": "workspace:*",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@tsed/common": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/mongoose": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/async": "3.2.15",
    "barrelsby": "^2.8.1",
    "eslint": "^8.12.0",
    "express": "^4.17.1",
    "formio": "3.4.1",
    "jest": "^29.2.0",
    "mongodb": "*",
    "mongoose": "^6.1.7"
  },
  "peerDependencies": {
    "@tsed/common": "7.62.2",
    "express": "^4.17.1",
    "formio": ">=2.0.0",
    "lodash": "^4.17.21",
    "mongodb": "*",
    "mongoose": "^6.1.7"
  },
  "peerDependenciesMeta": {
    "@tsed/common": {
      "optional": false
    }
  }
}