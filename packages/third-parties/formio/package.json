{
  "name": "@tsed/formio",
  "description": "Formio package for Ts.ED framework",
  "type": "module",
  "version": "8.0.0-beta.7",
  "keywords": [
    "Formio",
    "Ts.ED",
    "tsed",
    "TypeScript",
    "decorators",
    "express"
  ],
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "build": "yarn barrels && yarn build:ts",
    "barrels": "barrels",
    "start": "ts-node test/app/index.ts",
    "test": "vitest run",
    "build:ts": "tsc --build tsconfig.json",
    "test:ci": "vitest run --coverage.thresholds.autoUpdate=true"
  },
  "dependencies": {
    "@tsed/formio-types": "workspace:*",
    "@tsed/normalize-path": "workspace:*",
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@tsed/barrels": "workspace:*",
    "@tsed/core": "workspace:*",
    "@tsed/di": "workspace:*",
    "@tsed/mongoose": "workspace:*",
    "@tsed/platform-http": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/async": "3.2.24",
    "eslint": "9.12.0",
    "express": "^4.21.0",
    "formio": "4.2.5",
    "mongodb": "^6.9.0",
    "mongoose": "8.7.0",
    "typescript": "5.4.5",
    "vitest": "2.1.2"
  },
  "peerDependencies": {
    "@tsed/platform-http": "8.0.0-beta.7",
    "express": "^4.17.1",
    "formio": ">=2.0.0",
    "lodash": "^4.17.21",
    "mongodb": "*",
    "mongoose": "^8.7.0"
  },
  "peerDependenciesMeta": {
    "@tsed/common": {
      "optional": false
    }
  }
}
