{
  "name": "@tsed/mongoose-example",
  "version": "1.0.0",
  "description": "Here an example project with Mongoose and Ts.ED framework.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn clean && yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "yarn build && docker-compose build",
    "deploy": "exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "6.1.5",
    "@tsed/common": "6.1.5",
    "@tsed/core": "6.1.5",
    "@tsed/di": "6.1.5",
    "@tsed/exceptions": "6.1.5",
    "@tsed/schema": "6.1.5",
    "@tsed/json-mapper": "6.1.5",
    "@tsed/platform-express": "6.1.5",
    "@tsed/mongoose": "6.1.5",
    "@tsed/swagger": "6.1.5",
    "@tsed/testing-mongoose": "6.1.5",
    "@types/swagger-schema-official": "2.0.21",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "method-override": "^3.0.0",
    "mongoose": "5.9.27",
    "serve-static": "^1.13.1",
    "cross-env": "7.0.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/express": "4.17.7",
    "@types/http-proxy": "^1.17.1",
    "@types/mocha": "8.0.3",
    "@types/mongoose": "5.7.34",
    "@types/node": "14.11.1",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "9.0.5",
    "@types/sinon-chai": "3.2.4",
    "@types/supertest": "2.0.10",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "5.3.0",
    "mocha": "8.1.3",
    "nodemon": "1.19.4",
    "nyc": "15.1.0",
    "rimraf": "^3.0.0",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  }
}