{
  "name": "mongoose",
  "version": "1.0.0",
  "description": "Here an example project with Mongoose and Ts.ED framework.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "yarn build && docker-compose build",
    "deploy": "exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "6.10.3",
    "@tsed/common": "6.10.3",
    "@tsed/core": "6.10.3",
    "@tsed/di": "6.10.3",
    "@tsed/exceptions": "6.10.3",
    "@tsed/json-mapper": "6.10.3",
    "@tsed/mongoose": "6.10.3",
    "@tsed/platform-express": "6.10.3",
    "@tsed/schema": "6.10.3",
    "@tsed/swagger": "6.10.3",
    "@tsed/testing-mongoose": "6.10.3",
    "@types/swagger-schema-official": "2.0.21",
    "ajv": "6.12.6",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "method-override": "3.0.0",
    "mongoose": "5.10.11",
    "serve-static": "^1.13.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-jest": "2.0.0",
    "@tsed/cli-plugin-mongoose": "2.0.0",
    "@tsed/testing-mongoose": "6.10.3",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "@types/http-proxy": "^1.17.1",
    "@types/jest": "26.0.15",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "5.7.37",
    "@types/node": "14.14.6",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "concurrently": "5.3.0",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "rimraf": "^3.0.0",
    "supertest": "6.0.0",
    "ts-jest": "26.4.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  }
}